#ifndef SOUND_EFFECT_H_
#define SOUND_EFFECT_H_

#include "synthesizer.h"

typedef struct
{
  uint16_t note_count;
  uint16_t note_duration;
  square_note_t sq1_notes[];

} sound_t;

extern sound_t current_sound;

#endif /* SOUND_EFFECT_H_ */
